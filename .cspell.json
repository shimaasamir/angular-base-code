{
  "version": "0.2",
  "language": "en",
  "import": [
    "@cspell/dict-typescript",
    "@cspell/dict-software-terms",
    "@cspell/dict-node",
    "@cspell/dict-en_us",
    "@cspell/dict-en-gb",
    "@cspell/dict-npm",
    "@cspell/dict-html",
    "@cspell/dict-companies",
    "@cspell/dict-filetypes",
    "@cspell/dict-bash",
    "@cspell/dict-lorem-ipsum/cspell-ext.json",
    "@cspell/dict-es-es/cspell-ext.json"
  ],
  "allowCompoundWords": true,
  "flagWords": [],
  "ignorePaths": [
    ".eslintcache",
    ".stylelintcache",
    ".vscode/**",
    "dist/**",
    "node_modules/**",
    "package-lock.json",
    "package.json",
    "tsconfig.json",
    "tsconfig.build.json",
    "yarn.lock",
    "**/coverage/**",
    "**/node_modules/**",
    "**/dist/**",
    "**/fixtures/**",
    "**/public/**",
    "CHANGELOG.md",
    "**/changelog.mdx"
  ],
  "dictionaries": [
    "typescript",
    "softwareTerms",
    "node",
    "en_us",
    "en-gb",
    "npm",
    "html",
    "companies",
    "misc",
    "filetypes",
    "bash",
    "lorem-ipsum",
    "es-es"
  ],
  "words": [
    "apng",
    "avif",
    "Bienvenidx",
    "borjapazr",
    "browserslistrc",
    "commitlintrc",
    "czrc",
    "endent",
    "healthz",
    "htmlhintrc",
    "iisnode",
    "Jang",
    "joelwmale",
    "lcov",
    "lintstagedrc",
    "ncurc",
    "Neue",
    "ngfactory",
    "ngneat",
    "ngstyle",
    "ngsw",
    "nguniversal",
    "opsz",
    "OWASP",
    "robotstxt",
    "skyux",
    "sonarjs",
    "styl",
    "Suvorov",
    "Titillium",
    "Todoist",
    "Transloco",
    "versionrc",
    "webp",
    "wght"
  ]
}
